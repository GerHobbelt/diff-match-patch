{
  "name": "@sanity/diff-match-patch",
  "version": "0.0.8",
  "description": "Robust diff, match and patch algorithms to perform operations required for synchronizing plain text",
  "sideEffects": false,
  "main": "./lib/cjs/index.cjs",
  "source": "./src/index.ts",
  "module": "./lib/esm/index.js",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.cjs",
      "default": "./lib/esm/index.js"
    }
  },
  "engines": {
    "node": ">=14.16"
  },
  "type": "module",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc && tsc --module commonjs --outDir lib/cjs",
    "postbuild": "node scripts/fixExtensions.js",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "diff",
    "diff-match-patch",
    "google-diff-match-patch"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^14.0.0",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "testit": "^3.1.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
}
